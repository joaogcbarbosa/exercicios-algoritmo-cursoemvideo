Algoritmo "85"

Var

   vetNome: vetor[1..5] de caractere
   vetSal: vetor[1..5] de real
   vetSexo: vetor[1..5] de caractere
   i: inteiro
   nome, sexo: caractere
   sal: real

Inicio

      para i <- 1 até 5 faça
           EscrevaL("FUNCIONÁRIO ",i)
           Escreva("Nome: ")
           Leia(nome)
           vetNome[i] <- nome
           Escreva("Sexo[M/F]: ")
           Leia(sexo)
           vetSexo[i] <- sexo
           Escreva("Salário: R$")
           Leia(sal)
           vetSal[i] <- sal
      fimpara
      
      para i <- 1 até 5 faça
           se (vetSexo[i] = "F") entao
              se (vetSal[i] > 5000.00) entao
                 EscrevaL(vetNome[i]," recebe R$",vetSal[i])
              fimse
           fimse
      fimpara


Fimalgoritmo